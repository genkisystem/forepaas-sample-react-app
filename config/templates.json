{
  "recharts": {
    "options": {
      "font": "Source Sans Pro",
      "series": [
        {
          "fill": "rgba(0, 204, 249, 0.5)",
          "stroke": "#0089C0"
        }
      ],
      "barCategoryGap": 10,
      "cartesianGrid": {
        "stroke": "#f5f5f5",
        "vertical": false
      },
      "legend": {
        "hide": true,
        "height": 10,
        "verticalAlign": "top",
        "align": "right",
        "iconType": "circle",
        "iconSize": 9
      },
      "tooltip": {
        "style": {
          "padding": "10px",
          "backgroundColor": "rgba(62, 69, 80, 0.85)",
          "color": "white",
          "borderRadius": 10,
          "boxShadow": "inset 0 -3px 0 0 rgba(0,0,0,0.13)",
          "textAlign": "center"
        },
        "hideName": true,
        "style-label": {
          "fontSize": 11,
          "fontWeight": 600
        },
        "style-content": {
          "display": "flex",
          "justifyContent": "center"
        },
        "style-name": {
          "fontSize": 13
        },
        "style-value": {
          "fontSize": 13,
          "fontWeight": 600
        }
      },
      "xAxis": {
        "height": 20,
        "axisLine": false,
        "interval": 0,
        "tickLine": false,
        "tickStyle": {
          "y": -8,
          "fontSize": 12
        }
      },
      "yAxis": {
        "tickLine": false,
        "axisLine": false,
        "tickStyle": {
          "x": -24
        }
      }
    }
  },
  "recharts.donut": {
    "options": {
      "colors": [
        "#00CCF9",
        "#B18BFF",
        "#9EDF10",
        "#FF6F90",
        "#FACD30",
        "#61DDA8",
        "#0089C0"
      ],
      "series": [
        {
          "interval": 0,
          "labelLine": false,
          "stroke": null,
          "fill": null,
          "innerRadius": 50,
          "outerRadius": 60
        }
      ]
    }
  },
  "recharts.bar": {
    "options": {
      "xAxis": {
        "tickStyle": {
          "fontSize": 10
        }
      }
    }
  },
  "recharts.bar-horizontal": {
    "options": {
      "barSize": 6,
      "yAxis": {
        "hide": true
      },
      "xAxis": {
        "tickStyle": {
          "y": 0
        }
      }
    }
  },
  "recharts.scatter": {
    "extends": "recharts",
    "options": {
      "xAxis": {
        "tickStyle": {
          "y": -10,
          "fontSize": 12
        },
        "labelStyle": {
          "fill": "#97A7B7",
          "fontSize": 14,
          "offset": -25
        }
      },
      "yAxis": {
        "labelStyle": {
          "fill": "#97A7B7",
          "fontSize": 14,
          "angle": -92,
          "position": "left",
          "offset": -2
        }
      }
    }
  },
  "recharts.line": {
    "extends": "recharts"
  }
}