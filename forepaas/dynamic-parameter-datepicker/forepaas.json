{
  "name": "Simple Datepicker",
  "version": "2.2.0",
  "description": "A simple datepicker",
  "id": "dynamic-parameter-datepicker",
  "type": "dynamic-parameter",
  "main": "index.js",
  "displays": [
    {
      "id": "datepicker",
      "name": "Datepicker range",
      "category": "datepicker",
      "icon": "fpui fpui-calendar",
      "options": {
        "type": {
          "type": "hidden",
          "value": "filter"
        },
        "reference": {
          "tooltip": "dynamic_parameter.reference.tooltip",
          "label": "dynamic_parameter.reference",
          "type": "select-qb-filter-range"
        },
        "ranges": {
          "label": "label.ranges",
          "type": "datepicker-select-ranges"
        },
        "minDate": {
          "label": "label.mindate",
          "type": "datepicker-ranges"
        },
        "maxDate": {
          "label": "label.maxdate",
          "type": "datepicker-ranges"
        },
        "startAt": {
          "label": "label.startat",
          "type": "datepicker-ranges"
        },
        "endAt": {
          "label": "label.endat",
          "type": "datepicker-ranges"
        }
      }
    },
